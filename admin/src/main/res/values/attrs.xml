        <attr name="btnBottom" format="integer|reference" />
        <attr name="btnFrame" format="integer|reference" />
        <attr name="btnMask" format="integer|reference" />
        <attr name="btnUnpressed" format="integer|reference" />
        <attr name="btnPressed" format="integer|reference" />
    </declare-styleable>
    <declare-styleable name="PassCodeView">
        <attr name="numDigits" format="integer"/>
        <attr name="digitElevation" format="dimension"/>
